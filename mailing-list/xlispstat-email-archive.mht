(defmeth wkde-proto :do-mode-testing-click (x y m1 m2)
"when click, identify the mode, draw coordinates and lines
 and mark the 'show-modes with the index of the mode
 then call the :test-a-mode kde-proto method"
  (kde-require "modetest" "modetest")
  (let* ((rect (send self :content-rect))
         (xyscaled (send self :canvas-to-scaled x y))
         finalxy
         (xclic (first xyscaled))
         modl index finalindex curr (diff 9e999))
   (when (and (< (first rect) x (+ (first rect) (third rect)))
              (< (second rect) y (+ (second rect) (fourth rect))))
     (setf modl (second (send self :to-core :mode-list)))
     (dotimes (it (length modl))
     	(setf curr (abs (- (select modl it) xclic)))
     	(when (< curr diff)
     	      (setf index it)
              (setf diff curr)))
     (setf finalxy 
       (apply #'send self :do-show-coordinates
  	      (append (send self :scaled-to-canvas (select modl index)
                                                   (second xyscaled))
                      (list m1 m2))))
     ;;check that mouse is still in the mode
     (setf xclic
           (first (apply #'send self :canvas-to-scaled finalxy)))
     (setf diff 9e99)
     (dotimes (it (length modl))
     	(setf curr (abs (- (select modl it) xclic)))
     	(when (< curr diff)
     	      (setf finalindex it)
              (setf diff curr)))
     (when (= index finalindex)
           (send self :to-core :slot-value 'show-modes index)
           (send self :to-core :test-a-mode  index :print t)
           index)
)))
          



