;;; --------------------------
;;; test 3-d line drawing ...
;;; --------------------------
;; scale factor for test -- set b to any number 
( setf b 2 )

( setf S ( spin-function 
           #'(lambda (x y) (+ -1 (* x x) (* y y)))
           (- b) b (- b) b :color 'green ))


( defmeth s :do-click ( x y a b )
  ( let (( scaled ( send self :canvas-to-scaled x y )))
    ( format T "CANVAS: ~A    SCALED: ~A   C->Real: ~A\n"
             ( list x y )
             scaled 
             ( send self :canvas-to-real x y )))
  ( when ( and a b )
         ( send self :zline x y ))
  ( call-next-method x y a b ))


( defmeth s :zline ( cx cy &key (color 'red))
  ( let* (( real-xy ( send self :canvas-to-real cx cy ))
          ( real-x ( car real-xy ))
          ( real-y ( cadr real-xy ))
          ( transmat ( send self :transformation ))
          ( shifts ( send self :shift ( send self :current-variables )))
