( defproto ref-name-list-proto '( fit-model names elems colors) () 
name-list-proto )
 
 
( defmeth ref-name-list-proto :isnew ( fitmod )
  ( let (( u ( send fitmod :unk ))
         ( r ( send fitmod :refs )))
    ( call-next-method 0 :title ( send u :name ))
    ( setf ( slot-value 'fit-model ) fitmod )
    ( setf ( slot-value 'elems )
                        ( mapcar #'(lambda (x) (send x :elem)) r ))
    ( setf ( slot-value 'names )
                        ( mapcar #'(lambda (x) (send x :name)) r ))
   ( setf ( slot-value 'colors ) 
          ( send ( send color-list :new ) :slot-value 'colors )) 
   ( send self :add-points (length r))
    ( send self :point-label (iseq (length r)) ( slot-value 'names ))
    ( send self :showing-labels T )
    ( send ( send u :slot-value 'plot ) :add-subordinate self )
    ( send self :redraw )))

( defmeth ref-name-list-proto :do-click ( &rest args )
  ( apply #'call-next-method args )
  ( let* (( n ( car ( send self :selection )))
          ( c ( elt ( slot-value 'colors ) n ))
          ( fit ( slot-value 'fit-model )))
    ( let (( r  ( elt ( send fit :refs ) n ))
           ( a  ( elt ( send fit :coef-estimates ) n ))
           ( p  ( send ( send fit :unk ) :slot-value 'plot )))
      ( if ( < n ( send self :num-points ))
           ( progn 
             ( send self :draw-color c )
             ( send ( * r a ) :add-lines p :color c :width 2 )
             ( elem-label   p
                            ( elt ( slot-value 'elems ) n ) :color c ))
           ( send self :redraw )))))




