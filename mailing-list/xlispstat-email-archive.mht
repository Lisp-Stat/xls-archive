 [ "The grass is always greener, except at t=0" - Stan Kelly-Bootle ]


( defun guarded-parse-color (name)
  ( let (( result ( parse-color name )))
    ( if result result
         ( error "Not in color table: ~S" ( string name )))))


( defun color-complement ( color-symbol &optional new-color )
  "Args: (COLOR-SYMBOL &optional NEW-COLOR)
   makes color complement of COLOR-SYMBOL. Returns new-color-symbol.
   If a new-color symbol is not given, it interns the new color as 
   '*-complement' (*=old-color) and returns that symbol." 
  ( let (( new-color  ( if new-color new-color 
                           ( intern 
                             ( concatenate 'string
                                           ( string color-symbol )
                                           "-complement" )))))
    ( apply #'make-color new-color  
            ( - 1 ( guarded-parse-color color-symbol )))
    new-color ))

( defun darker-color ( color-symbol &optional new-color  (dec 0.2))
  "Args: ( color-symbol &optional new-color (dec 0.2))
   Makes and returns symbol for a new color darker that the old one."
    ( let (( new-color  ( if new-color new-color 
                           ( intern 
                             ( concatenate 'string
                                           "DARK-"
                                           ( string color-symbol ))))))
      ( apply #'make-color new-color
              ( mapcar #'(lambda (x) (max x 0.0))
                       (-  ( guarded-parse-color color-symbol ) dec)))
              new-color ))

( defun lighter-color ( color-symbol &optional new-color  (inc 0.2))
  "Args: ( color-symbol &optional new-color (inc 0.2))
   Makes and returns symbol for a new color lighter that the old one."
    ( let (( new-color  ( if new-color new-color 
                           ( intern 
                             ( concatenate 'string
                                           "LIGHT-"
                                           ( string color-symbol ))))))
      ( apply #'make-color new-color
             ( mapcar #'(lambda (x) (min x 1.0)) 
                      (+  (guarded-parse-color color-symbol) inc)))
              new-color ))
                                           
( defun free-extra-colors ()
  "Args: ()
   Frees all (color-symbols) not in *colors*" 
  ( dolist ( c (color-symbols))
           ( if ( not ( find c *colors* ))
                ( free-color c )))
  (color-symbols))




