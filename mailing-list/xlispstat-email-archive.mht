>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>;;;;                                                                       
>;;;;  A subset of the pull-and-move.lsp program.  Shows the projection
>;;;;  onto a line that the user may move around.  The projection is
>;;;;  based on a specific loss function where the independant variable
>;;;;  is normally distributed.  The regression line may also be inserted.
>;;;;                                                                       
>;;;;  
>;;;;                                                                       
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>;;
>;;  Prototype Definitions
>;;
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>
>(defproto move-line-mix '(plot info reg reg-coefs reg-ss xproj yproj inprod
>                          your-coefs your-rss k-value xx yy xbar ybar 
>                          sdx sdy prin-dir))
>
>(defproto move-line-proto '(parent) nil scatterplot-proto)
>(defproto info-proto '(parent) () graph-window-proto)
>
>(defun move-line (&optional (x (normal-rand 5)) (y (normal-rand 5)))
>  (let ((move (send move-line-mix :new x y)))
>    move))
>
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>;;
>;;  :isnew method
>;;
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>
>(defmeth move-line-mix :isnew (x y)
> (let* (
>       (plot (send move-line-proto :new 2 :title "Pull My Line"
>              :location
>              #+unix (list 50 50)
>              #-unix (list 50 50)
>       ))
>       (info-window (send info-proto :new :location
>              #+unix (list 500 50)
>              #-unix (list 300 50)
>                                   :size
>              #+unix (list 450 150)
>              #-unix (list 450 150)
>       ))
>       (plot-save-image (send menu-item-proto :new "Save to File"
>                     :action #'(lambda () (send plot :project-points)
>                                          (send plot :ask-save-image))))
>       (info-save-image (send menu-item-proto :new "Save to File"
>                     :action #'(lambda () (send info-window :ask-save-image))))
>       (which-type (send menu-item-proto :new "Display Which Projections?"
>                         :action #'(lambda () (get-type self)
>                                              (if (send self :reg)
>                                                  (send self :set-regression))
>                                              (send info-window :redraw)
>                                              (send plot :project-points))))
>       (plot-menu (send menu-proto :new "Point Pull"))
>       (info-window-menu (send menu-proto :new "Info Display"))
>       (xbar (mean x))
>       (ybar (mean y))
>       (svd (sv-decomp (matrix (list 2 2)
>               (list (sum (^ (- x xbar) 2)) (sum (* (- x xbar) (- y ybar)))
>                     (sum (* (- x xbar) (- y ybar))) (sum (^ (- y ybar) 2))))))
>       (pos (position (max (elt svd 1)) (elt svd 1)))
>       (prin-dir (elt (column-list (elt svd 2)) pos))
>      )
>     (send plot :parent self)
>     (send info-window :parent self)
>     (send plot :add-points (list x y))
>     (send plot :margin 0 25 0 0)
>     (send plot :add-overlay (send pull-overlay-proto :new))
>     (send self :xx (send plot :scaled-range 0))
>     (send self :yy (send plot :scaled-range 1))
>     (send self :your-rss 0)
>     (send self :reg-ss 0)
>     (send plot-menu :append-items which-type plot-save-image)
>     (send info-window-menu :append-items info-save-image)
>     (send plot :menu plot-menu)
>     (send info-window :menu info-window-menu)
>     (send plot :add-mouse-mode 'pulling :title "Pulling Mode" 
>                          :click :where-am-i :cursor 'hand)
>     (send self :xbar xbar)
>     (send self :ybar ybar)
>     (send self :sdx (standard-deviation x))
>     (send self :sdy (standard-deviation y))
>     (send self :inprod (inner-product (- x xbar) (- y ybar)))
>     (send self :prin-dir (/ (elt prin-dir 1) (elt prin-dir 0)))
>     (send plot :adjust-to-data)
>     (send self :plot plot)
>     (send self :info info-window)
>     (send self :begin-characteristics)
>     (send plot :mouse-mode 'pulling)))
>
>
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>;;
>;;  Line Animation Junk
>;;
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>
>(defmeth move-line-proto :where-am-i (x y m1 m2)
>(send self :while-button-down
>      #'(lambda (x y)
>          (let* (
>                 (parent (send self :parent))
>                 (xx (send parent :xx))
>                 (yy (send parent :yy))
>                 (cc (send self :canvas-to-scaled x y))
>                 (bg (list (first xx) (first yy)))
>                 (ed (list (second xx) (second yy)))
>                 (d1 (dist bg cc))
>                 (d2 (dist ed cc))
>                )
>            (cond ((< d1 d2) (setf (first xx) (first cc))
>                   (setf (first yy) (second cc)))
>              (t (setf (second xx) (first cc))
>                 (setf (second yy) (second cc))))
>          (send self :project-points)))))
>
>
>(defmeth move-line-proto :project-points ()
>(let* (
>       (parent (send self :parent))
>       (xx (send parent :xx))
>       (yy (send parent :yy))
>       (n (send self :num-points))
>       (x (send self :point-coordinate 0 (iseq n)))
>       (y (send self :point-coordinate 1 (iseq n)))
>       (coefs (send parent :line-calculate xx yy))
>       (regtest (send parent :reg))
>      )
>  (unless regtest (send self :redraw))
>  (send parent :calc-params coefs)
>  (send self :clear-lines)
>  (when regtest (send self :draw-regline)
>                (send self :linestart-width (iseq 2) (repeat 2 2))
>                (send self :redraw))
>  (send self :add-lines xx yy)
>  (let ((xproj (send parent :xproj))
>        (yproj (send parent :yproj)))
>    (dotimes (i n) 
>      (send self :add-lines (list (elt x i) (elt xproj i))
>                            (list (elt y i) (elt yproj i)) :type 'dashed)))
> (send (send parent :info) :update-info)))
>
>
>
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>;;
>;;  Sets initial conditions.  Calculates and inserts regression line.
>;;
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>
>(defmeth move-line-mix :set-regression ()
>    (send self :calc-params nil)
>    (send (send self :info) :redraw))
>
>(defmeth move-line-mix :begin-characteristics ()
>    (send self :k-value 0)
>    (send self :calc-params nil)
>    (let ((xrange (send (send self :plot) :range 0))
>          (yrange (send (send self :plot) :range 1))
>          (coefs (send self :reg-coefs)))
>    (send self :xx xrange)
>    (send self :yy (+ (elt coefs 0) (* (elt coefs 1) xrange)))
>    (send self :reg nil)
>    (send (send self :info) :update-info)
>    (send (send self :plot) :project-points)))
>
>(defmeth move-line-mix :line-calculate (xx yy)
>  (let* (
>         (m (/ (- (second yy) (first yy)) (- (second xx) (first xx))))
>         (a (- (first yy) (* m (first xx))))
>        )
>   (send self :your-coefs (list a m))
>   (list a m)
>  )
>)
>
>
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>;;
>;;   Calculates Parameters
>;;
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>
>(defmeth move-line-mix :calc-params (coefs)
>  (let* (
>         (plot (send self :plot))
>         (n (send plot :num-points))
>         (x (send plot :point-coordinate 0 (iseq n)))
>         (y (send plot :point-coordinate 1 (iseq n)))
>         (projections (send self :find-proj x y coefs))
>        )
>    (if coefs (progn
>                (send self :your-coefs coefs)
>                (send self :xproj (elt (elt projections 1) 0))
>                (send self :yproj (elt (elt projections 1) 1))
>                (send self :your-rss (elt projections 2)))
>              (progn
>                (send self :reg-coefs (elt projections 0))
>                (send self :reg-ss (elt projections 2))))))
>
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>;;
>;;  Finds Projections
>;;  K=0 Vertical
>;;  K=1 Horizontal
>;;  K=2 Orthogonal
>;;
>;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
>
>
>(defmeth move-line-mix :find-proj (x y coefs)
> (let* (
>        (n (length x))
>        (k (send self :k-value))
>        (xbar (send self :xbar))
>        (ybar (send self :ybar))
>        (sdx (send self :sdx))
>        (sdy (send self :sdy))
>        (inprod (send self :inprod))
>        (sgn (if (< inprod 0) -1 1))
>        (beta (if coefs (elt coefs 1)
>                        (case k (0 (/ inprod (* (1- n) (^ sdx 2))))
>                                (1 (/ (* (1- n) (^ sdy 2)) inprod))
>                                (2 (send self :prin-dir)))))
>        (alpha (if coefs (elt coefs 0) 
>                         (case k (0 (- ybar (* beta xbar)))
>                                 (1 (* (- (- xbar (* (/ beta) ybar))) beta))
>                                 (2 (- ybar (* beta xbar))))))
>        (xproj (case k (0 x)
>                       (1 (- (/ y beta) (/ alpha beta)))
>                       (2 (* (/ beta (1+ (^ beta 2)))
>                             (+ y (/ x beta) (- alpha))))))
>
>        (yproj (case k (0 (+ alpha (* beta x)))
>                       (1 y)
>                       (2 (+ alpha (* beta xproj)))))
>        (ss (sum (+ (^ (- x xproj) 2) (^ (- y yproj) 2))))
>       )
>   (list (list alpha beta) (list xproj yproj) ss)))
>
>
>(defmeth move-line-proto :draw-regline ()
>   (let* (
>          (parent (send self :parent))
>          (coefs (send parent :reg-coefs))
>          (xrange (send self :range 0))
