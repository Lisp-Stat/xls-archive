;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define a global true for readability
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(setf true t)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; define a global false for readability
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(setf false nil)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define a nil predicate for readability
;; PARAMETERS
;;	Value	- the value to be tested
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun Nil?(pValue)
"Args: (pValue)
Returns true if pValue is nil, otherwise false"
	(eq nil pValue)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define a list type predicate for readability
;; PARAMETERS
;;	Value	- the value to be tested
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun List?(pValue)
"Args: (pValue)
Returns true if pValue is a list, otherwise false"
	(listp pValue)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define a number type predicate for readability
;; PARAMETERS
;;	Value	- the value to be tested
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun Number?(pValue)
"Args: (pValue)
Returns true if pValue is a number, otherwise false"
	(numberp pValue)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define a real number type predicate for readability
;; PARAMETERS
;;	Value	- the value to be tested
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun Real?(pValue)
"Args: (pValue)
Returns true if pValue is a real, otherwise false"
	(realp pValue)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define an integer number type predicate for readability
;; PARAMETERS
;;	Value	- the value to be tested
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun Integer?(pValue)
"Args: (pValue)
Returns true if pValue is an integer, otherwise false"
	(integerp pValue)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define a natural number type predicate for readability
;; PARAMETERS
;;	Value	- the value to be tested
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun Natural?(pValue)
"Args: (pValue)
Returns true if pValue is a natural number, otherwise false"
	(and (integerp pValue) (>= pValue 0))
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define a string type predicate for readability
;; PARAMETERS
;;	Value	- the value to be tested
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun String?(pValue)
"Args: (pValue)
Returns true if pValue is a string, otherwise false"
	(stringp pValue)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Define a character type predicate for readability
;; PARAMETERS
;;	Value	- the value to be tested
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun Character?(pValue)
"Args: (pValue)
Returns true if pValue is a character, otherwise false"
	(if (symbolp pValue)
		(= 1 (length (symbol-name pValue)))
