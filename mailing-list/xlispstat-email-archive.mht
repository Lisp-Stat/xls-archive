;;; LINKABLE.LSP 

;;; linkable object Mixin 


( defproto linkable-proto '( links linked ) () )


( defmeth linkable-proto :linked ( &optional ( value nil set ))
  ( if set ( slot-value 'linked value )
       ( if ( slot-value 'linked ) ( send self :links ))))

( defmeth linkable-proto :links ( &optional ( value nil set ))
  ( if set ( slot-value 'links value )
       ( slot-value 'links )))


( defmeth linkable-proto :add-link (obj)
  ( let (( links ( send self :links )))
    ( if ( not ( find obj links :test #'eq ))
         ( send self :links ( cons obj links )))))

( defmeth linkable-proto :remove-link (obj)
  ( send self :links  
         ( remove obj ( send self :links ) :test #'eq )))

( defmeth linkable-proto :broadcast ( &rest msg ) 
  ( dolist ( other ( send self :links  ))
           ( if ( send other :has-method ( car msg ))
                ( apply #'send other msg ))))

( provide "LINKABLE" )



