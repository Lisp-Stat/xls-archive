;; alias for compound-data-seq 

( setf ( symbol-function 'seq )
       ( function compound-data-seq ))

( setf ( documentation 'seq 'function ) 
       ( documentation 'compound-data-seq 'function ))


;; To make compound-data-sequences "plug-replacible" with built-in Lisp 
sequences,
;; we have to redefine several built-in functions to work on either. 


;; LENGTH :: shadowed alias for compound-data-length 

( setf ( symbol-function 'length )
       ( function compound-data-length ))

( setf ( documentation 'length 'function )
       ( documentation 'compound-data-length 'function ))

;; ELT :: redefine shadowed ELT to work on built-in Lisp or compound-data 
sequences.

( defun elt ( sequence n )
"Args: (a i) 
 Returns element I of Lisp sequence or compound-data-sequence A"
  ( LISP::ELT ( seq sequence ) n ))

;; SEQUENCEP 

( setf ( symbol-function 'sequencep )
       ( function compound-data-p ))

( setf ( documentation 'sequencep 'function )
       ( documentation 'compound-data-p 'function ))

;; ... we also need a setter macro so ELT can continue to be used with 
SETF forms

( defsetf elt ( a i ) ( val )
