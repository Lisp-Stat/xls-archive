;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to generate a list of random numbers
;; PARAMETERS
;;	N		- the number of elements in output list, must be
greater than 0
;;	Range 	- random numbers will be between 0 and Range, if integer
entered
;;			  then a list of integers is generated, if real
then a list of
;;			  reals
;;
;; RETURNS
;;	list
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun RandomSequence(pN pRange)
"Args: (pN pRange)
Returns a list of pN random numbers in the range 0 - pRange"
	(if (and (natural? pN) (number? pRange) (< 0 pN))
	    (let	(	(output '())
	    		)
	      		(dotimes(i pN output)
	        		(setf output (cons (random pRange) output))
	     	)
	    )
		(DomsErr "Generals.lsp" "RandomSequence" "Bad parameters")
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to test if a value is in a list
;; PARAMETERS
;;	Value	- the value to be searched for in the list
;;	List	- the list to be searched, must be a list
;;
;; RETURNS
;;	boolean
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Member test required to catch strings

(defun InList?(pValue pList)
"Args: (pValue pList)
Returns true if an item of value pValue is in pList, otherwise false"
	(if (list? pList)
		(not (empty? (member pValue pList :test #'equal)))
		false
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to return the index of an element if it is in a list,
;; indexes begin at 0
;; PARAMETERS
;;	Value	- the value to be searched for
;;	List	- the list to be searched, must be a list
;;
;; RETURNS
;;	number
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun GetElementIndex(pValue pList &optional(pIndex 0))
"Args: (pValue pList)
Returns the index of the first item of value pValue in pList, or nil if
the item is not in the list.
Note: Indexing begins at index 0"
	(if (list? pList)
		(let(	(tail (member pValue pList :test #'equal))
			)
			(if tail
				(- (length pList) (length tail))
				nil
			)
		)
		(DomsErr "Generals.lsp" "GetElementIndex"

"Parameter must be a list")
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to get a value from a list given its index, indexes begin at 0
;; PARAMETERS
;;	List	- the list to be searched, must be a list
;;	Index	- the index of the element
;;
;; RETURNS
;;	element
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun GetElement(pList pIndex)
"Args: (pList pIndex)
Returns the item at index pIndex in pList, or nil if
the item is not in the list.
Note: Indexing begins at index 0"
	(if (and (list? pList) (natural? pIndex))
		(nth pIndex pList)
		(DomsErr "Generals.lsp" "GetElement" "Bad parameters")
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Destructive Function to set a value in a list given its index,
;; indexes begin at 0
;; PARAMETERS
;;	List	- the list to be searched, must be a list
;;	Index	- the index of the element
;;	Value	- the new value of the element
;;
;; RETURNS
;;	List
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun SetElement(pInList pIndex pValue)
"Args: (pList pIndex pValue)
Returns a list similar to pList but with item at index pIndex set to pValue.
Note: Indexing begins at index 0"
	(cond
		((and (list? pInList) (natural? pIndex) (> (length pInList)
pIndex))
			(setf (nth pIndex pInList) pValue)
			pInList
		)
		(true
			(DomsErr "Generals.lsp" "SetElement" "Bad parameters")
		)
	)
)
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to add a value to a list
;; PARAMETERS
;;	List	- the list into which the value is stored, must be a list
;;	any		- the value to be added
;;
;; RETURNS
;;	List
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun AddElement(pList pValue)
"Args: (pList pValue)
Returns a list with pValue appended to pList"
	(if (list? pList)
		(append pList (list pValue))
		(DomsErr "Generals.lsp" "AddElement" "Bad parameters")
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to get last character from a string
;; PARAMETERS
;;	String	- the list to be searched, must be a string
;;
;; RETURNS
;;	char
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun GetStringTerminator(pString)
"Args: (pString)
Returns the last character of pString"
	(if (and (string? pString) (not (eq (length pString) 0)))
		(char pString (- (length pString) 1))
		(DomsErr "Generals.lsp" "GetStringTerminator"
								"Parameter
must be a string")
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to partially copy a string
;; PARAMETERS
;;	String	- the string to be copied, must be a string
;;	N		- the number of characters to copy, if greater than
;;			  the length of the string, the string only is returned
;;
;; RETURNS
;;	string
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun StrnCpy(pSrc pN &optional (pDest "") (pCount 0))
"Args: (pString pN)
Returns a string containing up to n characters of pString"
	(if (and (string? pSrc) (natural? pN))
		(if (or (eq pCount pN) (eq pCount (length pSrc)))
			pDest
			(StrnCpy pSrc pN
				(concatenate 'String pDest (string (char
pSrc pCount)))
				(+ pCount 1)
			)
		)
		(DomsErr "Generals.lsp" "StrnCpy" "Bad parameters")
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to concanenate n strings
;; PARAMETERS
;;	String...String
;;		strings to be concatenated together
;;
;; RETURNS
;;	string
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun StrCat(&rest pArgs)
"Args: (&rest pStrings)
Returns a string containing the contcatenation of all argument strings"
	(let(	(result "")
		)
		(if (some #'(lambda(x) (not (string? x))) pArgs)
			(DomsErr "Generals.lsp" "StrnCpy" "Bad parameters")
			(dolist(astr pArgs result)
				(setf result (concatenate 'String result astr))
			)
		)
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to subtract 2 strings
;; PARAMETERS
;;	String1 String2
;;		string2 to be subtracted from string1
;;
;; RETURNS
;;	string
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun StrDiff(pStr1 pStr2)
"Args: (pStr1 pStr2)
Returns a pStr1 less pStr2 if pStr1 is of form <*><pStr2>"
	(let(	(result pStr1)
			(lstr2 (length pStr2))
		)
		(if (or (not (string? pStr1)) (not (string? pStr2)))
			(DomsErr "Generals.lsp" "StrnCpy" "Bad parameters")
			(dotimes(i lstr2 result)
				(setf result
					(string-right-trim
						(tostring (char pStr2 (-
lstr2 (+ i 1))))
						result
					)
				)
			)
		)
	)
)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to remove the last character from a string
;; PARAMETERS
;;	String	- the string to be modified, must be a string. If
;;			  the string is empty it returns an empty string
;;
;; RETURNS
;;	string
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun RemoveStringTerminator(pString)
"Args: (pString)
Returns a string less the last character of the string"
	(if (string? pString)
		(if (eq pString "")
			""
			(StrnCpy pString (- (length pString) 1))
		)
		(DomsErr "Generals.lsp" "RemoveStringTerminator"
								"Parameter
must be a string")
	)
)



