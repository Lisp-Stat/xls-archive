( free-extra-colors )   ; start fresh (optional) defined in colors.lsp 

;; remove some colors from the set of all in colortable
;; ( i.e. the grays and the standard fixed colors )


( setf colors 
       ( remove nil 
            ( mapcar #'(lambda (x) (if (find x *colors*) nil x))
                     ( mapcar #'intern 
                                ( remove "GRAY" 
                                      ( remove "GREY"  
					 ( mapcar #'car *colortable* )
                                           :test #'string-search)
                                 :test #'string-search )))))

;; and shuffle them so we will have a random selection 
( setf colors ( shuffle colors ))


( add-color 'gray )
( add-color 'lightgray )

;; fill the color table  -- we'll get an error when the table is full 
( unwind-protect 
( ignore-errors ( dolist ( c colors )
                         ( add-color ( print c ))))
)

( setf colors (color-symbols))

( setf colors-used 
       ( mapcar #'(lambda (x) (find x colors)) 
                ( mapcar #'intern ( mapcar #'car *colortable* ))))

( setf s ( spin-plot 
           ( transpose ( mapcar #'cdr *colortable* ))
           :point-labels ( mapcar #'car *colortable* )))

( send s :use-color T )

;; color the points whose color is in the allocated (color-symbols) table 
( send s :point-color (iseq (length colors-used)) colors-used)


;; selected points show up with larger dots 
;; the uncolored points will show up smaller and dimmer. 
( send s :point-selected ( iseq ( length colors-used )) colors-used )




