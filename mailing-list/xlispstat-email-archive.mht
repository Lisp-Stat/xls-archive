#include <LowMem.h>	/* sdm7g */

#define SetSFCurVol( val ) LMSetSFSaveDisk( (short) val )	/* sdm7g */
#define SetSFCurDir( val ) LMSetCurDirStore( (long) val )	/* sdm7g */

int	chdir(char *path)
{
  WDPBRec pb;
 	
  pb.ioCompletion	= nil;
  pb.ioNamePtr	= nil;
  if ((dd_errno = PBHGetVol(&pb, false)) != noErr)
    return -1;

  strcpy(buf, path);
  CtoPstr(buf);
  pb.ioNamePtr = (StringPtr) buf;
  dd_errno = PBHSetVol(&pb, false);
  
  SetSFCurVol( pb.ioVRefNum );	/* sdm7g  shadow in Standard File Dialogs */
  SetSFCurDir( pb.ioWDDirID );	/* sdm7g */

  return dd_errno ? -1 : 0;
}



