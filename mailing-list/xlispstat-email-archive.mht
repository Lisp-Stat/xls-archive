;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun FlattenTree(pExp &optional(pResult nil))
	(if (nil? pExp)
		pResult
		(if (List? (car pExp))
			(append (append pResult (FlattenTree (car pExp)))

	(FlattenTree (cdr pExp)))
			(append (append pResult (list (car pExp)))

	(Flattentree (cdr pExp)))
		)
	)
)

