;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
;; Function to remove dupliacates from a list of elements
;; eg (1 2 3 3 4 4 5) -> (1 2 3 4 5)
;; PARAMETERS
;;	List - A list of elements, possibly containing duplicates
;;
;; RETURNS
;;	List - List of unique elements in same order as input
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
(defun UniqueList(pList)
"Args: (pList)
Returns a list containing each unique element in pList"
(let(	(outlist '())
	    )
  		(dolist(element pList nil)
    		(setf outlist
				(if (not (inlist? element outlist))
					(AddElement outlist element)
    				outlist
    			)
    		)
  		)
  		outlist
  	)
)

