  (declare (type fixnum ic)) (declare (type fixnum i))
  (declare (type fixnum idp2)) (declare (type fixnum k))
  (fdo ((k 1 (+ k 1))) ((> k l1) nil)
   (tagbody (fset (fref ch 1 k 1) (+ (fref cc 1 1 k) (fref cc ido 2 k)))
    (fset (fref ch 1 k 2) (+ (fref cc 1 1 k) (- (fref cc ido 2 k))))
  ))
  (arithmetic-if (+ ido (- 2)) (go label107) (go label105) (go label102))
  label102 (setf idp2 (+ ido 2)) (if (< (/ (+ ido (- 1)) 2) l1) (go label108))
  (fdo ((k 1 (+ k 1))) ((> k l1) nil)
   (tagbody
    (fdo ((i 3 (+ i 2))) ((> i ido) nil)
     (tagbody (setf ic (+ idp2 (- i)))
      (fset (fref ch (+ i (- 1)) k 1)
       (+ (fref cc (+ i (- 1)) 1 k) (fref cc (+ ic (- 1)) 2 k))
      )
      (setf tr2 (+ (fref cc (+ i (- 1)) 1 k) (- (fref cc (+ ic (- 1)) 2 k))))
      (fset (fref ch i k 1) (+ (fref cc i 1 k) (- (fref cc ic 2 k))))
      (setf ti2 (+ (fref cc i 1 k) (fref cc ic 2 k)))
      (fset (fref ch (+ i (- 1)) k 2)
       (+ (* (fref wa1 (+ i (- 2))) tr2) (* (* -1 (fref wa1 (+ i (- 1)))) ti2))
      )
      (fset (fref ch i k 2)
       (+ (* (fref wa1 (+ i (- 2))) ti2) (* (fref wa1 (+ i (- 1))) tr2))
  )))))
  (go label111) label108
  (fdo ((i 3 (+ i 2))) ((> i ido) nil)
   (tagbody (setf ic (+ idp2 (- i)))
    (fdo ((k 1 (+ k 1))) ((> k l1) nil)
     (tagbody
      (fset (fref ch (+ i (- 1)) k 1)
       (+ (fref cc (+ i (- 1)) 1 k) (fref cc (+ ic (- 1)) 2 k))
      )
      (setf tr2 (+ (fref cc (+ i (- 1)) 1 k) (- (fref cc (+ ic (- 1)) 2 k))))
      (fset (fref ch i k 1) (+ (fref cc i 1 k) (- (fref cc ic 2 k))))
      (setf ti2 (+ (fref cc i 1 k) (fref cc ic 2 k)))
      (fset (fref ch (+ i (- 1)) k 2)
