  label111 (if (= (mod ido 2) 1) (go end_label)) label105
  (fdo ((k 1 (+ k 1))) ((> k l1) nil)
   (tagbody (fset (fref ch ido k 1) (+ (fref cc ido 1 k) (fref cc ido 1 k)))
    (fset (fref ch ido k 2) (- (+ (fref cc 1 2 k) (fref cc 1 2 k))))
